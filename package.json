{
    "name": "vue-generator-graph",
    "displayName": "vue-generator-graph",
    "description": "An app which will generate a graph of your entire vue application",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:visualize.start"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "visualize.start",
                "title": "Vizualize your vue app ðŸ˜‰",
                "category": "Vizualizer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "yarn run test-compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.55.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "ts-loader": "^9.2.3",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0",
        "webpack": "^5.44.0",
        "webpack-cli": "^4.7.2"
    },
    "dependencies": {
        "change-case": "^4.1.2",
        "es-module-lexer": "^0.6.0",
        "html-tags": "^3.1.0",
        "mermaid": "^8.11.0",
        "node-html-parser": "^4.0.0"
    }
}
